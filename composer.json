{
  "name": "iteaoffice/calendar",
  "type": "library",
  "license": "proprietary",
  "description": "Code to manage calendar",
  "keywords": [
    "calendar"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.itea3.eu"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.2",
    "zendframework/zend-mvc": "^3.0.1",
    "zendframework/zend-permissions-acl": "2.7.*",
    "kokspflanze/zfc-twig": "2.2.*",
    "zendframework/zend-mvc-form": "1.0.*",
    "zendframework/zend-mvc-console": "1.2.*",
    "zendframework/zend-mvc-plugins": "1.0.*",
    "setasign/fpdi-tcpdf": "^2.0",
    "doctrine/doctrine-orm-module": "2.1.*",
    "iteaoffice/contact": "3.0.*",
    "iteaoffice/project": "3.0.*"
  },
  "require-dev": {
    "iteaoffice/testing": "1.1.*"
  },
  "autoload": {
    "psr-4": {
      "Calendar\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CalendarTest\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev",
      "dev-develop": "3.1.x-dev"
    }
  }
}
